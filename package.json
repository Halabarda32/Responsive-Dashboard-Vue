		"sass": "~1.32.0",
		"sass-loader": "^10.0.0",
		"vue-cli-plugin-vuetify": "~2.5.8",
		"vue-jest": "^3.0.7",
		"vue-template-compiler": "^2.6.14",
		"vuetify-loader": "^1.7.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"jest/globals": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended",
			"plugin:jest/recommended"
		],
		"parserOptions": {
			"parser": "@babel/eslint-parser"
		},
		"rules": {},
		"overrides": [
			{
				"files": [
					"**/__tests__/*.{j,t}s?(x)",
					"**/tests/unit/**/*.spec.{j,t}s?(x)"
				],
				"env": {
					"jest": true
				}
			}
		],
		"plugins": [
			"jest"
		]
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "vue-jest",
			".*\\.(js)$": "babel-jest"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
